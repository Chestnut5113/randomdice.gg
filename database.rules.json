{
    "rules": {
        ".write": false,
        "auth_test": {
            ".read": false,
            ".write": "auth.token.isEditor === true"
        },
        "last_updated": {
            ".read": true,
            ".write": "auth.token.isEditor === true",
            "patreon_list": {
                ".write": "auth.uid !== null || auth.uid === 'my-service-worker'"
            }
        },
        "decks": {
            ".read": true,
            ".write": "auth.token.isEditor === true"
        },
        "decks_guide": {
            ".read": true,
            ".write": "auth.token.isEditor === true"
        },
        "dice": {
            ".read": true,
            ".write": "auth.token.isEditor === true"
        },
        "wiki": {
            ".read": true,
            ".write": "auth.token.isEditor === true"
        },
        "news": {
            ".read": true,
            ".write": "auth.token.isEditor === true"
        },
        "token_storage": {
            ".read": "auth.uid === 'my-service-worker'",
            ".write": "auth.uid === 'my-service-worker'"
        },
        "credit": {
            ".read": true,
            ".write": "auth.token.isEditor === true"
        },
        "users": {
            ".read": "auth.uid === 'my-service-worker'",
            ".write": "auth.uid === 'my-service-worker'",
            "$uid": {
                ".read": "auth.uid === $uid || auth.uid === 'my-service-worker'",
                ".write": "auth.uid === $uid",
                "editor": {
                    ".write": false
                },
                "linked-account": {
                    ".write": "auth.uid === 'my-service-worker'"
                },
                "patreon-tier": {
                    ".write": "auth.uid === 'my-service-worker'"
                }
            }
        },
        "patreon_list": {
            ".read": true,
            ".write": "auth.uid === 'my-service-worker'",
            "$i": {
                "$uid": {
                    "message": {
                        ".write": "auth.uid === $uid"
                    },
                    "youtubeId": {
                        ".write": "auth.uid === $uid"
                    }
                }
            }
        }
    }
}